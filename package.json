{
    "name": "velocity-template-language",
    "displayName": "Velocity Template Language",
    "description": "Apache Velocity Template Language Syntax Highlighting",
    "version": "0.3.0",
    "publisher": "sodatea",
    "repository": {
        "type": "git",
        "url": "https://github.com/sodatea/vscode-velocity.git"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:velocity"
    ],
    "contributes": {
        "languages": [
            {
                "id": "velocity",
                "aliases": [
                    "Velocity",
                    "velocity"
                ],
                "extensions": [
                    ".vm",
                    ".fhtml",
                    ".vtl"
                ]
            }
        ],
        "grammars": [
            {
                "language": "velocity",
                "scopeName": "text.velocity",
                "path": "./syntaxes/Velocity.tmLanguage"
            },
            {
                "language": "velocity",
                "scopeName": "text.html.velocity",
                "path": "./syntaxes/VelocityHTML.tmLanguage"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pack": "vsce package"
    },
    "bugs": {
        "url": "https://github.com/sodatea/vscode-velocity/issues"
    },
    "homepage": "https://github.com/sodatea/vscode-velocity#readme",
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/vscode": "1.25.0",
        "@vscode/vsce": "^2.19.0",
        "tslint": "^5.11.0",
        "typescript": "^2.6.1"
    },
    "keywords": [
        "vtl",
        "vm",
        "velocity",
        "template",
        "markup"
    ],
    "icon": "velocity_logo.png",
    "galleryBanner": {
        "color": "#333399",
        "theme": "dark"
    }
}
